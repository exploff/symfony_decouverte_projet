<html>
<head>
    <title>{% block title %} {{ "title" | trans }} {% endblock %}</title>
    <meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
    <link rel="stylesheet" href="{{ asset('bundles/club/css/main.css') }}" type="text/css" media="all" />
    <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.1.0/jquery.min.js" type="text/javascript"></script>
</head>
<body>
    <div id="page">
        <div id="header">
            <img src="{{ asset('bundles/club/images/logo.png') }}" alt="Club" width="128" height="128"/>
            <ul id="menu">
                <li><a href="{{ path('club_accueil') }}">{{ "accueil" | trans }}</a></li>
                <li><a href="{{ path('club_club_lister') }}">{{ "club" | trans }}</a></li>
                <li><a href="{{ path('club_activite_lister') }}">{{ "activite" | trans }}</a></li>
                <li><a href="{{ path('club_adherent_lister') }}">{{ "adherent" | trans }}</a></li>
            </ul>
        </div>
        <div id="content">
            <h1>{{ block('title') }}</h1>
            {% block content %}
            {% endblock %}
        </div>
        <div id="footer">
            <p>{{ "copyright" | trans }}</p>
        </div>
    </div>
</body
</html>