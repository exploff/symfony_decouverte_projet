{% extends '@Club/layout.html.twig' %}
{% block title %}{{ "club.ajouter.modifier" | trans }}{% endblock %}
{% block content %}
    {{ form_start(form) }}

        <div class="error">{{ form_errors(form) }}</div>
        <table>
            <tr>
                <td> {{ form_label(form.nom, "club.nom" | trans) }}</td>
            </tr>
            <tr>
                <td> {{ form_errors(form.nom) }} {{ form_widget(form.nom) }}</td>
            </tr>

        </table>
        {{ form_rest(form) }}

    {{ form_end(form) }}

{% endblock %}


