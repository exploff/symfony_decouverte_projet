{% extends '@Agence/layout.html.twig' %}
{% block title %}{{ "proprietaire.ajouter.modifier" | trans }}{% endblock %}

{% block content %}
        {{ form_start(form) }}

        <div class="error">{{ form_errors(form) }}</div>
        <table>
            <tr>
                <td> {{ form_label(form.nom, "proprietaire.titre.nom" | trans ) }}</td>
                <td> {{ form_label(form.prenom, "proprietaire.titre.prenom" | trans ) }}</td>
            </tr>
            <tr>
                <td> {{ form_errors(form.nom) }} {{ form_widget(form.nom) }}</td>
                <td> {{ form_errors(form.prenom) }} {{ form_widget(form.prenom) }}</td>
            </tr>

        </table>
        {{ form_rest(form) }}

    {{ form_end(form) }}
{% endblock %}