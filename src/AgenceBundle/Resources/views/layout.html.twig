<html>
<head>
    <title>{% block title %} {{ "title" | trans }} {% endblock %}</title>
    <meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
    <link rel="stylesheet" href="{{ asset('bundles/agence/css/main.css') }}" type="text/css" media="all" />
    <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.1.0/jquery.min.js" type="text/javascript"></script>
</head>
<body>
    <div id="page">
        <div id="header">
            <img src="{{ asset('bundles/agence/images/test.png') }}" alt="Visiteur" width="298" height="169"/>
            <ul id="menu">
                <li><a href="{{ path('agence_homepage') }}">{{ "accueil" | trans }}</a></li>
                <li><a href="{{ path('agence_visiteur_accueil') }}">{{ "visiteur" | trans }}</a></li>
                <li><a href="{{ path('agence_typebien_accueil') }}">{{ "typebien" | trans }}</a></li>
                <li><a href="{{ path('agence_bien_accueil') }}">{{ "bien" | trans }}</a></li>
                <li><a href="{{ path('agence_proprietaire_accueil') }}">{{ "proprietaire" | trans }}</a></li>
                <li><a href="{{ path('agence_achat_accueil') }}">{{ "achat" | trans }}</a></li>
            </ul>
        </div>
        <div id="content">
            <h1>{{ block('title') }}</h1>
            {% block content %}
            {% endblock %}
        </div>
        <div id="footer">
            <p>{{ "copyright" | trans }}</p>
        </div>
    </div>
</body
</html>